{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "iam:CreateRole",
        "iam:PutRolePolicy",
        "iam:List*",
        "iam:Get*",
        "iam:Delete*",
        "iam:Pass*",
        "iam:Attach*",
        "iam:Upload*"
      ],
      "Resource": "*"
    }
  ]
}